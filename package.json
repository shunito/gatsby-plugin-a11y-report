  "author": "Shunsuke Ito <shunsuke.ito@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "homepage": "https://github.com/shunito/gatsby-plugin-a11y-report",
  "repository": {
    "type": "git",
    "url": "https://github.com/shunito/gatsby-plugin-a11y-report.git"
  },
  "license": "MIT",
  "scripts": {
