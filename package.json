{
  "name": "gatsby-plugin-a11y-report",
  "version": "1.1.0",
  "description": "Gatsby plugin for accessibility check",
  "main": "index.js",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "accessibility",
    "axe"
